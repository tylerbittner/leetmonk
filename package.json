{
  "name": "leetmonk",
  "version": "1.0.1",
  "description": "Offline LeetCode practice app",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "dist": "electron-vite build && electron-builder",
    "test": "jest --testPathPattern='tests/' --runInBand",
    "test:problems": "jest --testPathPattern='tests/problems'",
    "test:solutions": "jest --testPathPattern='tests/solutions'",
    "test:e2e": "bash tests/e2e.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1",
    "electron-vite": "^5.0.0",
    "jest": "^29.7.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^2.0.13",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.0"
  },
  "build": {
    "appId": "com.leetmonk.app",
    "productName": "LeetMonk",
    "mac": {
      "identity": null,
      "target": "dmg"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "data/**/*"
    ],
    "extraResources": [
      {
        "from": "data",
        "to": "data"
      }
    ]
  }
}
